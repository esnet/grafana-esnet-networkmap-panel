{
  "name": "esnet-networkmap-panel",
  "version": "2.0.5",
  "description": "Network Map Panel",
  "repository": "https://github.com/esnet/grafana-esnet-networkmap-panel",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build_dts": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx @grafana/sign-plugin@latest --yes",
    "start": "yarn watch",
    "test": "karma start --single-run --browsers ChromeHeadless",
    "test:ci": "karma start --single-run --browsers ChromeHeadless",
    "typecheck": "tsc --noEmit"
  },
  "author": "ESnet",
  "email": "esnet-grafana-plugins@lbl.gov",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/data": "10.4.0",
    "@grafana/e2e": "10.4.0",
    "@grafana/e2e-selectors": "10.4.0",
    "@grafana/eslint-config": "7.0.0",
    "@grafana/runtime": "10.4.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@grafana/ui": "10.4.0",
    "@playwright/test": "1.34.3",
    "@swc/core": "1.3.51",
    "@swc/helpers": "0.5.6",
    "@testing-library/react": "12.1.4",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.19",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "6.10.0",
    "emotion": "11.0.0",
    "eslint-webpack-plugin": "4.0.1",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "glob": "^10.3.10",
    "grafana-plugin-support": "latest",
    "identity-obj-proxy": "3.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-should": "^1.0.0",
    "mocha": "^10.0.0",
    "prettier": "3.2.5",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.70.0",
    "sass-loader": "13.3.2",
    "should": "^2.0.2",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.3",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "5.3.3",
    "webpack": "5.90.2",
    "webpack-cli": "5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "main": "index.js",
  "types": "dist/exports.d.ts",
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "d3": "^7.0.0",
    "leaflet": "^1.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.4.1"
  }
}
