{
  "name": "esnet-networkmap-panel",
  "version": "2.0.7",
  "description": "Network Map Panel",
  "repository": "https://github.com/esnet/grafana-esnet-networkmap-panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build_dts": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx @grafana/sign-plugin@latest --yes",
    "start": "yarn watch",
    "test": "karma start --single-run --browsers ChromeHeadless",
    "test:react": "jest --watch --onlyChanged",
    "test:ci": "karma start --single-run --browsers ChromeHeadless",
    "typecheck": "tsc --noEmit"
  },
  "author": "ESnet",
  "email": "esnet-grafana-plugins@lbl.gov",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/data": "8.3.0",
    "@grafana/e2e": "9.3.8",
    "@grafana/e2e-selectors": "9.3.8",
    "@grafana/eslint-config": "7.0.0",
    "@grafana/runtime": "8.3.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@grafana/ui": "8.3.0",
    "@playwright/test": "1.34.3",
    "@swc/core": "1.3.51",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "release-12.x",
    "@types/jest": "^29.5.0",
    "@types/lodash": "4.14.187",
    "@types/node": "18.11.9",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "^5.14.8",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.7.1",
    "emotion": "11.0.0",
    "eslint-plugin-jest": "^28.8.2",
    "eslint-webpack-plugin": "3.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "glob": "^10.0.0",
    "grafana-plugin-support": "latest",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-should": "^1.0.0",
    "mocha": "^10.0.0",
    "prettier": "2.7.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.56.0",
    "sass-loader": "13.1.0",
    "should": "^2.0.2",
    "style-loader": "3.3.1",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.6.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "main": "index.js",
  "types": "dist/exports.d.ts",
  "engines": {
    "node": ">=15"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "d3": "^7.0.0",
    "leaflet": "^1.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.4.1"
  }
}
