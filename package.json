{
  "name": "esnet-networkmap-panel",
  "version": "2.0.3",
  "description": "Network Map Panel",
  "repository": "https://github.com/esnet/grafana-esnet-networkmap-panel",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode module && vite build --mode grafana",
    "build_dts": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache --report-unused-disable-directives --ignore-path ./.gitignore --max-warnings 0",
    "preview": "vite preview",
    "old_build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "old_dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "lint:fix": "yarn run lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx @grafana/sign-plugin@latest --yes",
    "start": "yarn watch",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "old_test": "karma start --single-run --browsers ChromeHeadless",
    "old_test:ci": "karma start --single-run --browsers ChromeHeadless",
    "typecheck": "tsc --noEmit"
  },
  "author": "ESnet",
  "email": "esnet-grafana-plugins@lbl.gov",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@grafana/data": "8.3.0",
    "@grafana/e2e": "9.3.8",
    "@grafana/e2e-selectors": "9.3.8",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/runtime": "8.3.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@grafana/ui": "8.3.0",
    "@swc/core": "^1.3.51",
    "@swc/helpers": "^0.5.0",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/browser": "^1.3.1",
    "css-loader": "^6.7.1",
    "emotion": "10.0.27",
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.0.0",
    "grafana-plugin-support": "latest",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "^24.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-should": "^1.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.5.0",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "should": "^2.0.2",
    "style-loader": "3.3.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "webdriverio": "^8.32.3"
  },
  "main": "index.js",
  "types": "dist/exports.d.ts",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "d3": "^7.0.0",
    "leaflet": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "2.5.0",
    "vitest": "^1.3.1"
  },
  "packageManager": "yarn@1.22.19"
}
